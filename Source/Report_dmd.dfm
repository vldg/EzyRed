  object qryRESIDENCE_LOT: TFDQuery
    MasterSource = dsRESIDENCE_ADDRESS
    Connection = dmdMain.dbMain
    SQL.Strings = (
      'SELECT REL.*, '
      '  T.T_SMALLNAME, CU.CU_NAME, CU.CU_FIRSTNAME, CU.CU_EMAIL,'
      '  LE.LE_ID'
      'FROM RESIDENCE_LOT REL'
      'LEFT OUTER JOIN LOT_EVENT LE ON (LE.REL_ID = REL.REL_ID)'
      'LEFT OUTER JOIN CUSTOMER CU ON (CU.CU_ID = LE.CU_ID)'
      'LEFT OUTER JOIN TITLE T ON (CU.T_ID = T.T_ID)'
      'WHERE REL.REA_ID =:REA_ID'
      'AND LE.LE_TYPE = 0'
      'AND LE.LE_END IS NULL')
    Left = 80
    Top = 328
    ParamData = <
      item
        Name = 'REA_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
  end
